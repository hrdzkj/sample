<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="LockPatternView">
        <attr name="aspect" format="string" />
    </declare-styleable>

    <declare-styleable name="LooperTextView">
        <attr name="loopertextSize" format="dimension"/>
        <attr name="loopertextColor" format="color|reference"/>
    </declare-styleable>


    <declare-styleable name="CustomTextView">
        <attr name="customFont">
            <enum name="brands" value="0" />
            <enum name="regular" value="1" />
            <enum name="solid" value="2" />
        </attr>
    </declare-styleable>
</resources>